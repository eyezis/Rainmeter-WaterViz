[MeasureAudio]
Measure=Plugin
Plugin=AudioLevel
Port=Output
FFTSize=16384
FFTOverlap=16000
FFTAttack=0
FFTDecay=0
FreqMin=25
FreqMax=10000
Sensitivity=30
Bands=129

[LuaRebuilder]
Measure=Script
ScriptFile=#CURRENTPATH#Rebuilder.lua
[LuaController]
Measure=Script
ScriptFile=#CURRENTPATH#Controller.lua
[MeasureSeconds]
Measure=Time
Format="%S"
[MeasureFpsLua]
Measure=Script
ScriptFile="#CURRENTPATH#fps.lua"
Disabled=1

[MeterFPS]
Meter=String
Text=""
StringAlign=LEFT
StringStyle=BOLD
StringEffect=SHADOW
FontEffectColor=0,0,0,200
FontColor=FFFFFF
FontSize=10
AntiAlias=1
